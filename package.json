{
  "name": "test-runz-app",
  "version": "1.0.0",
  "private": true,
  "description": "test-runz-app",
  "author": "vengatavaradhan",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "env-cmd -f .env.development gatsby develop",
    "start": "env-cmd -f .env.development gatsby develop",
    "start:prod": "env-cmd -f .env.production gatsby develop",
    "build": "gatsby build && npm run sentry:sourcemaps",
    "build:prod": "env-cmd -f .env.production gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "jest",
    "coverage": "jest --coverage",
    "precommit": "npm run lint && npm test",
    "prepare": "husky install",
    "invalidate": "aws cloudfront  create-invalidation --distribution-id EAFMPHESQM3W2 --paths '/*'",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org twilight-ny --project testrunz-develop ../../../.. && sentry-cli sourcemaps upload --org twilight-ny --project testrunz-develop ../../../.."
  },
  "dependencies": {
    "@apollo/client": "^3.8.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@influxdata/influxdb-client": "^1.33.2",
    "@loadable/component": "^5.16.3",
    "@mui/icons-material": "^5.14.9",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.10",
    "@mui/x-date-pickers": "^6.15.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/cli": "^2.28.5",
    "@sentry/react": "^7.100.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/react-beautiful-dnd": "^13.1.5",
    "apexcharts": "^3.45.2",
    "aws-sdk": "^2.1510.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "firebase": "^10.5.0",
    "formik": "^2.4.5",
    "gatsby": "^5.12.4",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-react-redux-providers": "^1.0.9",
    "gatsby-plugin-sass": "^6.12.0",
    "gatsby-source-graphql": "^5.12.0",
    "html-react-parser": "^5.0.7",
    "html2canvas": "^1.4.1",
    "html2json": "^1.0.2",
    "jspdf": "^2.5.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-calendar": "^4.6.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^6.1.0",
    "react-redux": "^8.1.3",
    "react-split-pane": "^2.0.3",
    "react-toastify": "^9.1.3",
    "reactjs-social-login": "^2.6.3",
    "recharts": "^2.9.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.67.0",
    "socket.io-client": "^4.7.2",
    "yup": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/loadable__component": "^5.13.8",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-color": "^3.0.7",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.1",
    "prettier": "3.0.3",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
